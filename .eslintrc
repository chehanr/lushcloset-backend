{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended",
    "plugin:jest/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "indent": [2, 2, { "SwitchCase": 1 }]
  },
  "env": {
    "jest/globals": true
  }
}
